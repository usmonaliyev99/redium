{
  "name": "usmonaliyev/redium",
  "description": "A high-performance laravel package powered by redis, inspired by sanctum.",
  "keywords": [
    "usmonaliyev",
    "laravel",
    "auth",
    "redis",
    "redium",
    "sanctum"
  ],
  "homepage": "https://github.com/usmonaliyev/redium",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Usmonaliyev Temur",
      "email": "t.usmonaliyev99@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "illuminate/auth": "^10|^11|^12",
    "illuminate/contracts": "^10|^11|^12",
    "illuminate/http": "^10|^11|^12",
    "illuminate/support": "^10|^11|^12"
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "pestphp/pest": "^4.1"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Usmonaliyev\\Redium\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Usmonaliyev\\Redium\\RediumServiceProvider"
      ],
      "aliases": {
        "Redium": "Redium"
      }
    }
  }
}
